
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

@await Html.PartialAsync("/Views/MemberLayoutPartial/_header.cshtml") 

<body class="g-sidenav-show  bg-gray-100">
    @await Html.PartialAsync("/Views/MemberLayoutPartial/_navbar.cshtml")
    <main class="main-content position-relative max-height-vh-100 h-100 mt-1 border-radius-lg ">
        <!-- Navbar -->
        @await Html.PartialAsync("/Views/MemberLayoutPartial/_navbartop.cshtml")

        <!-- End Navbar -->
        <div class="container-fluid py-4">

            @RenderBody()
            @await Html.PartialAsync("/Views/MemberLayoutPartial/_footer.cshtml")

        </div>
    </main>
    @await Html.PartialAsync("/Views/MemberLayoutPartial/_rightbar.cshtml")


    @await Html.PartialAsync("/Views/MemberLayoutPartial/_script.cshtml")

</body>

</html>
